# OpenAI API Configuration (required for CodeAgent AI and vulnerability analysis)
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Configuration (optional, for private repositories)
GITHUB_TOKEN=your_github_token_here

# AI Model Selection (for CodeAgent AI)
AI_MODEL=GPT_4  # Options: GPT_4, GPT_3_5_TURBO, GPT_4_32K

# Scanner Integration Settings
ENABLE_AI_ANALYSIS=true
AI_ANALYSIS_MIN_SEVERITY=high
MAX_CONCURRENT_AI_REVIEWS=1
AI_ANALYSIS_TIMEOUT_SEC=300

